instance_egress_security_rules:
  # Allow ssh connections outside
  - destination: "{{ quad_zero_route }}"
    protocol: "{{ TCP_protocol }}"
    tcp_options:
      destination_port_range:
        min: {{ SSH_port }}
        max: {{ SSH_port }}
  - destination: '0.0.0.0/0'
    protocol: 'all'
    is_stateless: True

